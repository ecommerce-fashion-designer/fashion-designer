        crossorigin="anonymous" referrerpolicy="no-referrer" />
    <link
        href="https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;600;700;800&family=Roboto&family=Source+Serif+4:opsz,wght@8..60,300;8..60,400;8..60,500;8..60,600&display=swap"
        rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/css/jass.css">
</head>

<body>
    <section class="header d-none d-md-block">
        <div class="container contianer_padding">
            <div class="row">
                <div class="col-xl-6 col-md-6 d-none d-md-block">
                    <div class="contact-us"> Contact :<a href="/" title="email Us"> 404@pagenotfound.com
                        </a>
                        <span> /</span>
                        <a href="/" title="call Us"> 404404404</a>
                    </div>
                </div>
                <div class="col-xl-6 col-md-6 col-12 text-end">
                    <ul class="right-header text-right">

                        <li>
                            {{#if loggedIn}}
                            <button id="logout">LOGOUT</button>
                            {{else}}
                            <a href="/login" title="Login"> LOGIN </a>
                            {{/if}}
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </section>

    <nav class="x header d-none d-md-block">
        <div class="container contianer_padding">
            <div class="row">
                <div class="col-xl-6 col-md-6 d-none d-md-block">
                    <a href="/" title="home"> HOME</a>
                    <span> /</span>
                    <a href="/dashboard" title="dashboard"> DASHBOARD</a>
                    <span> /</span>
                    <a href="/product" title="products"> PRODUCTS</a>
                    <span> /</span>
                    <a href="/cart" title="cart"> CART</a>
                    <span> /</span>
                    <a href="/checkout" title="checkout"> CHECKOUT</a>
                </div>
    </nav>




    <section>
        <div class="container-fluid g-0">
            {{{ body }}}
        </div>
    </section>

    <footer class="footer bg-dark">
        <div class="container g-0">
            <div class="row py-4">
                <div class="col-3">
                    <ul>
                        <li class="bold-list">COLLECTIONS:</li>
                        <li><a class="text-white-50" href="/category/men">Men</a></li>
                        <li><a class="text-white-50" href="/category/women">Women</a></li>
                        <li><a class="text-white-50" href="/category/kids">Kids</a></li>
                    </ul>
                </div>
                <div class="col-3">
                    <ul>
                        <li class="bold-list">CUSTOMER SERVICE:</li>
                        <li><a class="text-white-50" href="#">Contact Us</a></li>
                        <li><a class="text-white-50" href="#">Delivery</a></li>
                        <li><a class="text-white-50" href="#">Returns</a></li>
                        <li><a class="text-white-50" href="#">Size Guide</a></li>
                    </ul>
                </div>
                <div class="col-3 m-auto">
                    <img src="/images/icons/delivery.png" alt="Delivery" width="auto" height="70px">
                    <p class="bold-list mt-2">FREE STANDARD DELIVERY >$100</p>
                </div>
                <div class="col-3 m-auto">
                    <img src="/images/icons/returns.png" alt="Returns" width="auto" height="70px">
                    <p class="bold-list mt-2">30-DAY FREE RETURNS</p>
                </div>
            </div>
        </div>
    </footer>

    {{#if loggedIn}}
